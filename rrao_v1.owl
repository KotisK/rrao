    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://www.w3.org/2001/XMLSchema#duration -->

    <rdfs:Datatype rdf:about="http://www.w3.org/2001/XMLSchema#duration"/>
    


    <!-- http://www.w3.org/2001/XMLSchema#monetary -->

    <rdfs:Datatype rdf:about="http://www.w3.org/2001/XMLSchema#monetary"/>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- https://w3id.org/rrao#decides -->

    <owl:ObjectProperty rdf:about="https://w3id.org/rrao#decides">
        <rdfs:domain rdf:resource="https://w3id.org/rrao#court"/>
        <rdfs:range rdf:resource="https://w3id.org/rrao#courtDecision"/>
        <rdfs:label xml:lang="en">decides</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/rrao#defines -->

    <owl:ObjectProperty rdf:about="https://w3id.org/rrao#defines">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
        <rdfs:domain rdf:resource="https://w3id.org/rrao#courtDecision"/>
        <rdfs:range rdf:resource="https://w3id.org/rrao#penalty"/>
        <rdfs:comment xml:lang="en">Represents a court defining or establishing a penalty for a case.</rdfs:comment>
        <rdfs:label xml:lang="en">defines</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/rrao#executedAs -->

    <owl:ObjectProperty rdf:about="https://w3id.org/rrao#executedAs">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
        <rdfs:domain rdf:resource="https://w3id.org/rrao#penalty"/>
        <rdfs:range rdf:resource="https://w3id.org/rrao#executionOfPenalty"/>
        <rdfs:comment xml:lang="en">Indicates the manner in which a penalty or punishment is carried out.</rdfs:comment>
        <rdfs:label xml:lang="en">executed as</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/rrao#hasGravity -->

    <owl:ObjectProperty rdf:about="https://w3id.org/rrao#hasGravity">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
        <rdfs:domain rdf:resource="https://w3id.org/rrao#crime"/>
        <rdfs:range rdf:resource="https://w3id.org/rrao#gravityOfCrime"/>
        <rdfs:comment xml:lang="en">Indicates the severity or seriousness of a crime.</rdfs:comment>
        <rdfs:label xml:lang="en">has gravity</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/rrao#hasLevelOfParticipation -->

    <owl:ObjectProperty rdf:about="https://w3id.org/rrao#hasLevelOfParticipation">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
        <rdfs:domain rdf:resource="https://w3id.org/rrao#senetenced"/>
        <rdfs:range rdf:resource="https://w3id.org/rrao#levelOfParticipation"/>
        <rdfs:comment xml:lang="en">Describes the level or degree of participation an offender had in a crime.</rdfs:comment>
        <rdfs:label xml:lang="en">has level of participation</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/rrao#hasLiability -->

    <owl:ObjectProperty rdf:about="https://w3id.org/rrao#hasLiability">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
        <rdfs:domain rdf:resource="https://w3id.org/rrao#senetenced"/>
        <rdfs:range rdf:resource="https://w3id.org/rrao#liability"/>
        <rdfs:comment xml:lang="en">Represents the level of legal responsibility or liability an offender holds.</rdfs:comment>
        <rdfs:label xml:lang="en">has liability</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/rrao#hasRiskOfReoffending -->

    <owl:ObjectProperty rdf:about="https://w3id.org/rrao#hasRiskOfReoffending">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
        <rdfs:domain rdf:resource="https://w3id.org/rrao#senetenced"/>
        <rdfs:range rdf:resource="https://w3id.org/rrao#riskOfReoffending"/>
        <rdfs:comment xml:lang="en">Represents the assessed risk that an offender will reoffend or engage in criminal behavior again.</rdfs:comment>
        <rdfs:label xml:lang="en">has risk of reoffending</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/rrao#hasStatus -->

    <owl:ObjectProperty rdf:about="https://w3id.org/rrao#hasStatus">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
        <rdfs:domain rdf:resource="https://w3id.org/rrao#imprisoned"/>
        <rdfs:range rdf:resource="https://w3id.org/rrao#convictionStatus"/>
        <rdfs:comment xml:lang="en">Describes the current status of an offender in prison</rdfs:comment>
        <rdfs:label xml:lang="en">has status</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/rrao#improsonedAt -->

    <owl:ObjectProperty rdf:about="https://w3id.org/rrao#improsonedAt">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
        <rdfs:domain rdf:resource="https://w3id.org/rrao#imprisoned"/>
        <rdfs:range rdf:resource="https://w3id.org/rrao#prison"/>
        <rdfs:comment xml:lang="en">Indicates the prison where an offender is currently held.</rdfs:comment>
        <rdfs:label xml:lang="en">imprisoned at</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/rrao#isRelatedToCrime -->

    <owl:ObjectProperty rdf:about="https://w3id.org/rrao#isRelatedToCrime">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
        <rdfs:domain rdf:resource="https://w3id.org/rrao#penalty"/>
        <rdfs:range rdf:resource="https://w3id.org/rrao#crime"/>
        <rdfs:comment xml:lang="en">Indicates the association between a penalty and a crime related to the case.</rdfs:comment>
        <rdfs:label xml:lang="en">is related to crime</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/rrao#participatedIn -->

    <owl:ObjectProperty rdf:about="https://w3id.org/rrao#participatedIn">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
        <rdfs:domain rdf:resource="https://w3id.org/rrao#senetenced"/>
        <rdfs:range rdf:resource="https://w3id.org/rrao#crime"/>
        <rdfs:comment xml:lang="en">Indicates that the offender participated in a crime.</rdfs:comment>
        <rdfs:label xml:lang="en">participated in</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/rrao#sentencedTo -->

    <owl:ObjectProperty rdf:about="https://w3id.org/rrao#sentencedTo">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
        <rdfs:domain rdf:resource="https://w3id.org/rrao#senetenced"/>
        <rdfs:range rdf:resource="https://w3id.org/rrao#penalty"/>
        <rdfs:comment>Indicates the penalty to which an offender has been sentenced.</rdfs:comment>
        <rdfs:label>sentenced to</rdfs:label>
    </owl:ObjectProperty>
    


