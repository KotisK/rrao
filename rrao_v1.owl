        <rdfs:domain rdf:resource="https://w3id.org/rrao#person"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment xml:lang="en">The age group that the person belongs to.</rdfs:comment>
        <rdfs:label xml:lang="en">age group</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- https://w3id.org/rrao#amountOfPecuniaryPenalty -->

    <owl:DatatypeProperty rdf:about="https://w3id.org/rrao#amountOfPecuniaryPenalty">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topDataProperty"/>
        <rdfs:domain rdf:resource="https://w3id.org/rrao#pecuniarySentence"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#monetary"/>
        <rdfs:comment xml:lang="en">Specifies the monetary amount of the pecuniary penalty.</rdfs:comment>
        <rdfs:label xml:lang="en">amount of pecuniary penalty</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- https://w3id.org/rrao#beneficialCalculation -->

    <owl:DatatypeProperty rdf:about="https://w3id.org/rrao#beneficialCalculation">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topDataProperty"/>
        <rdfs:domain rdf:resource="https://w3id.org/rrao#penalty"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#boolean"/>
        <rdfs:comment xml:lang="en">Indicates whether beneficial calculation was applied to the penalty.</rdfs:comment>
        <rdfs:label xml:lang="en">beneficial calculation</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- https://w3id.org/rrao#birthYear -->

    <owl:DatatypeProperty rdf:about="https://w3id.org/rrao#birthYear">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topDataProperty"/>
        <rdfs:domain rdf:resource="https://w3id.org/rrao#person"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#dateTime"/>
        <rdfs:comment xml:lang="en">Specifies the birth year of a person.</rdfs:comment>
        <rdfs:label xml:lang="en">birth year</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- https://w3id.org/rrao#clildren -->

    <owl:DatatypeProperty rdf:about="https://w3id.org/rrao#clildren">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topDataProperty"/>
        <rdfs:domain rdf:resource="https://w3id.org/rrao#person"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#boolean"/>
        <rdfs:comment>Indicates whether the person has children.</rdfs:comment>
        <rdfs:label xml:lang="en">children</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- https://w3id.org/rrao#dateOfCrime -->

    <owl:DatatypeProperty rdf:about="https://w3id.org/rrao#dateOfCrime">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topDataProperty"/>
        <rdfs:domain rdf:resource="https://w3id.org/rrao#crime"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#dateTime"/>
        <rdfs:comment xml:lang="en">Represents the date on which the crime occurred.</rdfs:comment>
        <rdfs:label xml:lang="en">date of crime</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- https://w3id.org/rrao#dateOfEnteringPrison -->

    <owl:DatatypeProperty rdf:about="https://w3id.org/rrao#dateOfEnteringPrison">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topDataProperty"/>
        <rdfs:domain rdf:resource="https://w3id.org/rrao#custodialPenalty"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#dateTime"/>
        <rdfs:comment xml:lang="en">Specifies the date when the individual entered prison.</rdfs:comment>
        <rdfs:label xml:lang="en">date of entering prison</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- https://w3id.org/rrao#dateOfExitingPrison -->

    <owl:DatatypeProperty rdf:about="https://w3id.org/rrao#dateOfExitingPrison">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topDataProperty"/>
        <rdfs:domain rdf:resource="https://w3id.org/rrao#custodialPenalty"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#dateTime"/>
        <rdfs:comment xml:lang="en">Indicates the date when the individual is expected to exit prison.</rdfs:comment>
        <rdfs:label xml:lang="en">date of exiting prison</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- https://w3id.org/rrao#daysInCustody -->

    <owl:DatatypeProperty rdf:about="https://w3id.org/rrao#daysInCustody">
        <rdfs:domain rdf:resource="https://w3id.org/rrao#custody"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#int"/>
        <rdfs:label xml:lang="en">days in custody</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- https://w3id.org/rrao#daysSinceRelease -->

    <owl:DatatypeProperty rdf:about="https://w3id.org/rrao#daysSinceRelease">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topDataProperty"/>
        <rdfs:domain rdf:resource="https://w3id.org/rrao#senetenced"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#int"/>
        <rdfs:comment xml:lang="en">Indicates the days since the person has been released form prison</rdfs:comment>
        <rdfs:label xml:lang="en">days since release</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- https://w3id.org/rrao#decisionCode -->

    <owl:DatatypeProperty rdf:about="https://w3id.org/rrao#decisionCode">
