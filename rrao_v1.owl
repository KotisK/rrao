        <rdfs:label xml:lang="en">level of education</rdfs:label>
        <rdfs:seeAlso rdf:resource="https://ilostat.ilo.org/methods/concepts-and-definitions/classification-education/"/>
    </owl:DatatypeProperty>
    


    <!-- https://w3id.org/rrao#locatedIn -->

    <owl:DatatypeProperty rdf:about="https://w3id.org/rrao#locatedIn">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topDataProperty"/>
