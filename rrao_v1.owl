        <rdfs:domain rdf:resource="https://w3id.org/rrao#courtDecision"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment xml:lang="en">Specifies the code or reference associated with a court decision.</rdfs:comment>
        <rdfs:label xml:lang="en">decision code</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- https://w3id.org/rrao#decisionDate -->

    <owl:DatatypeProperty rdf:about="https://w3id.org/rrao#decisionDate">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topDataProperty"/>
        <rdfs:domain rdf:resource="https://w3id.org/rrao#courtDecision"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#dateTime"/>
        <rdfs:comment xml:lang="en">Indicates the date associated with a court decision.</rdfs:comment>
        <rdfs:label xml:lang="en">decision date</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- https://w3id.org/rrao#generalRegisterNumber -->

    <owl:DatatypeProperty rdf:about="https://w3id.org/rrao#generalRegisterNumber">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topDataProperty"/>
        <rdfs:domain rdf:resource="https://w3id.org/rrao#senetenced"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment xml:lang="en">Represents the general register number associated with an imprisoned person.</rdfs:comment>
        <rdfs:label xml:lang="en">general register number</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- https://w3id.org/rrao#irrevocableConviction -->

    <owl:DatatypeProperty rdf:about="https://w3id.org/rrao#irrevocableConviction">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topDataProperty"/>
        <rdfs:domain rdf:resource="https://w3id.org/rrao#penalty"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#boolean"/>
