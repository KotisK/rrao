    <owl:Ontology rdf:about="https://w3id.org/rrao">
        <owl:imports rdf:resource="http://i-lab.aegean.gr/ontologies/rrao/iccs#"/>
        <owl:imports rdf:resource="http://www.w3.org/ns/odrl/2/"/>
        <dc:bibliographicCitation xml:lang="en">FAIR-PReSONS EU JUST project, Reoffending Risk Assesment Ontology,  Revision: v0.5.</dc:bibliographicCitation>
        <dc:contributor xml:lang="en">A. Sykiotou, Democritus University of Thace, asikioto@law.duth.gr</dc:contributor>
        <dc:contributor xml:lang="en">Joana Pinho, IPS, joana.pinho@prisonsystems.eu</dc:contributor>
        <dc:creator xml:lang="en">K. Kotis, i-lab, University of the Aegean, kotis@aegean.gr</dc:creator>
        <dc:creator xml:lang="en">S. Angelis, i-lab, University of the Aegean, sotiris@aegean.gr</dc:creator>
        <dc:description xml:lang="en">RRAO focuses on the comprehensive representation of knowledge of reoffending and related concepts such as, crime, penalty, court decision.
The ontology development is part of the FAIR – PReSONS research project.  https://fair-presons.aegean.gr/</dc:description>
        <dc:licence xml:lang="en">CC0</dc:licence>
        <dc:references xml:lang="en">eurovoc: https://op.europa.eu/en/web/eu-vocabularies/dataset/-/resource?uri=http://publications.europa.eu/resource/dataset/eurovoc</dc:references>
        <dc:references xml:lang="en">https://i-lab.aegean.gr/kotis/Ontologies/RRAO/iccs.owl</dc:references>
        <dc:references xml:lang="en">odrl:https://www.w3.org/ns/odrl/2/</dc:references>
        <dc:title xml:lang="en">Reoffending Risk Assessment Ontology</dc:title>
        <vann:preferredNamespacePrefix xml:lang="en">rrao</vann:preferredNamespacePrefix>
        <rdfs:label xml:lang="en">Reoffending Risk Assessment Ontology</rdfs:label>
        <rdfs:seeAlso xml:lang="en"></rdfs:seeAlso>
        <owl:versionInfo>v1</owl:versionInfo>
        <ns:term_status xml:lang="en"></ns:term_status>
