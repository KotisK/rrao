        <rdfs:label xml:lang="en">is from conversion</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- https://w3id.org/rrao#isInitialSentence -->

    <owl:DatatypeProperty rdf:about="https://w3id.org/rrao#isInitialSentence">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topDataProperty"/>
        <rdfs:domain rdf:resource="https://w3id.org/rrao#penalty"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#boolean"/>
        <rdfs:label xml:lang="en">is initial sentence</rdfs:label>
        <rdfs:seeAlso xml:lang="en">Indicates whether the penalty is an initial sentence.</rdfs:seeAlso>
    </owl:DatatypeProperty>
    


    <!-- https://w3id.org/rrao#levelOfEducation -->

    <owl:DatatypeProperty rdf:about="https://w3id.org/rrao#levelOfEducation">
