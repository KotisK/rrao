        <rdfs:label xml:lang="en">Misdemeanour</rdfs:label>
    </owl:Class>
    


    <!-- http://i-lab.aegean.gr/ontologies/rrao#nonCustodialPenalty -->

    <owl:Class rdf:about="http://i-lab.aegean.gr/ontologies/rrao#nonCustodialPenalty">
