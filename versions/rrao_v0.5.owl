        <rdfs:label xml:lang="en">Penal court</rdfs:label>
        <rdfs:seeAlso xml:lang="en">euvoc:criminalCourt</rdfs:seeAlso>
    </owl:Class>
    


    <!-- http://i-lab.aegean.gr/ontologies/rrao#penalty -->

    <owl:Class rdf:about="http://i-lab.aegean.gr/ontologies/rrao#penalty">
