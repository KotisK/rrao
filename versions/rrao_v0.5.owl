        <rdfs:seeAlso xml:lang="en">foaf:Person</rdfs:seeAlso>
    </owl:Class>
    


    <!-- http://i-lab.aegean.gr/ontologies/rrao#principalPerpetrator -->

    <owl:Class rdf:about="http://i-lab.aegean.gr/ontologies/rrao#principalPerpetrator">
