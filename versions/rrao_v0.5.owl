<?xml version="1.0"?>
<rdf:RDF xmlns="http://i-lab.aegean.gr/ontologies/rrao#"
     xml:base="http://i-lab.aegean.gr/ontologies/rrao"
     xmlns:dc="http://purl.org/dc/elements/1.1/"
     xmlns:ns="http://www.w3.org/2003/06/sw-vocab-status/ns#"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:wot="http://xmlns.com/wot/0.1/"
     xmlns:xml="http://www.w3.org/XML/1998/namespace"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:foaf="http://xmlns.com/foaf/spec/"
     xmlns:odrl="https://www.w3.org/ns/odrl/2/"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
     xmlns:skos="http://www.w3.org/2004/02/skos/core#"
     xmlns:foaf2="http://xmlns.com/foaf/0.1/"
     xmlns:odrl1="http://www.w3.org/ns/odrl/2/"
     xmlns:terms="http://purl.org/dc/terms/"
     xmlns:eurovoc="http://publications.europa.eu/resource/authority/eurovoc/">
    <owl:Ontology rdf:about="http://i-lab.aegean.gr/ontologies/rrao">
        <owl:imports rdf:resource="http://www.w3.org/ns/odrl/2/"/>
        <dc:contributor xml:lang="en">K. Kotis, i-lab, University of the Aegean, kotis@aegean.gr</dc:contributor>
        <dc:contributor xml:lang="en">S. Angelis, i-lab, University of the Aegean, sotiris@aegean.gr</dc:contributor>
        <dc:description xml:lang="en">RRAO focuses on the comprehensive representation of knowledge of reoffending and related concepts such as, crime, penalty, court decision. 
The ontology development is part of the FAIR – PReSONS research project.  https://fair-presons.aegean.gr/</dc:description>
        <dc:title xml:lang="en">Reoffending Risk Assesment Ontology</dc:title>
        <rdfs:label xml:lang="en">Reoffending Risk Assesment Ontology</rdfs:label>
        <owl:versionInfo>v0.5</owl:versionInfo>
        <ns:term_status xml:lang="en"></ns:term_status>
    </owl:Ontology>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Annotation properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://purl.org/dc/elements/1.1/contributor -->

    <owl:AnnotationProperty rdf:about="http://purl.org/dc/elements/1.1/contributor"/>
    


    <!-- http://purl.org/dc/elements/1.1/description -->

    <owl:AnnotationProperty rdf:about="http://purl.org/dc/elements/1.1/description"/>
    


    <!-- http://purl.org/dc/elements/1.1/title -->

    <owl:AnnotationProperty rdf:about="http://purl.org/dc/elements/1.1/title"/>
    


    <!-- http://www.w3.org/2003/06/sw-vocab-status/ns#term_status -->

    <owl:AnnotationProperty rdf:about="http://www.w3.org/2003/06/sw-vocab-status/ns#term_status"/>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Datatypes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://www.w3.org/2001/XMLSchema#duration -->

    <rdfs:Datatype rdf:about="http://www.w3.org/2001/XMLSchema#duration"/>
    


    <!-- http://www.w3.org/2001/XMLSchema#monetary -->

    <rdfs:Datatype rdf:about="http://www.w3.org/2001/XMLSchema#monetary"/>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://i-lab.aegean.gr/ontologies/rrao#defines -->

    <owl:ObjectProperty rdf:about="http://i-lab.aegean.gr/ontologies/rrao#defines">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
        <rdfs:domain rdf:resource="http://i-lab.aegean.gr/ontologies/rrao#courtDecision"/>
        <rdfs:range rdf:resource="http://i-lab.aegean.gr/ontologies/rrao#penalty"/>
        <rdfs:comment xml:lang="en">Represents a court defining or establishing a penalty for a case.</rdfs:comment>
        <rdfs:label xml:lang="en">defines</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://i-lab.aegean.gr/ontologies/rrao#executedAs -->

    <owl:ObjectProperty rdf:about="http://i-lab.aegean.gr/ontologies/rrao#executedAs">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
        <rdfs:domain rdf:resource="http://i-lab.aegean.gr/ontologies/rrao#penalty"/>
        <rdfs:range rdf:resource="http://i-lab.aegean.gr/ontologies/rrao#executionOfPenalty"/>
        <rdfs:comment xml:lang="en">Indicates the manner in which a penalty or punishment is carried out.</rdfs:comment>
        <rdfs:label xml:lang="en">executed as</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://i-lab.aegean.gr/ontologies/rrao#hasGravity -->

    <owl:ObjectProperty rdf:about="http://i-lab.aegean.gr/ontologies/rrao#hasGravity">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
        <rdfs:domain rdf:resource="http://i-lab.aegean.gr/ontologies/rrao#crime"/>
        <rdfs:range rdf:resource="http://i-lab.aegean.gr/ontologies/rrao#gravityOfCrime"/>
        <rdfs:comment xml:lang="en">Indicates the severity or seriousness of a crime.</rdfs:comment>
        <rdfs:label xml:lang="en">has gravity</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://i-lab.aegean.gr/ontologies/rrao#hasLevelOfParticipation -->

    <owl:ObjectProperty rdf:about="http://i-lab.aegean.gr/ontologies/rrao#hasLevelOfParticipation">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
        <rdfs:comment xml:lang="en">Describes the level or degree of participation an offender had in a crime.</rdfs:comment>
        <rdfs:label xml:lang="en">has level of participation</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://i-lab.aegean.gr/ontologies/rrao#hasLiability -->

    <owl:ObjectProperty rdf:about="http://i-lab.aegean.gr/ontologies/rrao#hasLiability">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
        <rdfs:domain rdf:resource="http://i-lab.aegean.gr/ontologies/rrao#senetenced"/>
        <rdfs:range rdf:resource="http://i-lab.aegean.gr/ontologies/rrao#liability"/>
        <rdfs:comment xml:lang="en">Represents the level of legal responsibility or liability an offender holds.</rdfs:comment>
        <rdfs:label xml:lang="en">has liability</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://i-lab.aegean.gr/ontologies/rrao#hasLikelihoodOfReoffending -->

    <owl:ObjectProperty rdf:about="http://i-lab.aegean.gr/ontologies/rrao#hasLikelihoodOfReoffending">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
        <rdfs:domain rdf:resource="http://i-lab.aegean.gr/ontologies/rrao#senetenced"/>
        <rdfs:range rdf:resource="http://i-lab.aegean.gr/ontologies/rrao#riskOfReoffending"/>
        <rdfs:comment xml:lang="en">Represents the assessed risk that an offender will reoffend or engage in criminal behavior again.</rdfs:comment>
        <rdfs:label xml:lang="en">has likelihood of recidivism</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://i-lab.aegean.gr/ontologies/rrao#hasPrisonStatus -->

    <owl:ObjectProperty rdf:about="http://i-lab.aegean.gr/ontologies/rrao#hasPrisonStatus">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
        <rdfs:domain rdf:resource="http://i-lab.aegean.gr/ontologies/rrao#imprisoned"/>
        <rdfs:range rdf:resource="http://i-lab.aegean.gr/ontologies/rrao#statusInPrison"/>
        <rdfs:comment xml:lang="en">Describes the current status of an offender in prison</rdfs:comment>
        <rdfs:label xml:lang="en">has prison status</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://i-lab.aegean.gr/ontologies/rrao#improsonedAt -->

    <owl:ObjectProperty rdf:about="http://i-lab.aegean.gr/ontologies/rrao#improsonedAt">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
        <rdfs:domain rdf:resource="http://i-lab.aegean.gr/ontologies/rrao#imprisoned"/>
        <rdfs:range rdf:resource="http://i-lab.aegean.gr/ontologies/rrao#prison"/>
        <rdfs:comment xml:lang="en">Indicates the prison where an offender is currently held.</rdfs:comment>
        <rdfs:label xml:lang="en">imprisoned at</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://i-lab.aegean.gr/ontologies/rrao#isRelatedToCrime -->

    <owl:ObjectProperty rdf:about="http://i-lab.aegean.gr/ontologies/rrao#isRelatedToCrime">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
        <rdfs:domain rdf:resource="http://i-lab.aegean.gr/ontologies/rrao#penalty"/>
        <rdfs:range rdf:resource="http://i-lab.aegean.gr/ontologies/rrao#crime"/>
        <rdfs:comment xml:lang="en">Indicates the association between a penalty and a crime related to the case.</rdfs:comment>
        <rdfs:label xml:lang="en">is related to crime</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://i-lab.aegean.gr/ontologies/rrao#orders -->

    <owl:ObjectProperty rdf:about="http://i-lab.aegean.gr/ontologies/rrao#orders">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
        <rdfs:domain rdf:resource="http://i-lab.aegean.gr/ontologies/rrao#courtDecision"/>
        <rdfs:range rdf:resource="http://i-lab.aegean.gr/ontologies/rrao#executionOfPenalty"/>
        <rdfs:comment xml:lang="en">Denotes the act of a court ordering the execution or enforcement of a penalty.</rdfs:comment>
        <rdfs:label xml:lang="en">orders</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://i-lab.aegean.gr/ontologies/rrao#participatedIn -->

    <owl:ObjectProperty rdf:about="http://i-lab.aegean.gr/ontologies/rrao#participatedIn">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
        <rdfs:domain rdf:resource="http://i-lab.aegean.gr/ontologies/rrao#senetenced"/>
        <rdfs:range rdf:resource="http://i-lab.aegean.gr/ontologies/rrao#crime"/>
        <rdfs:comment xml:lang="en">Indicates that the offender participated in a crime.</rdfs:comment>
        <rdfs:label xml:lang="en">participated in</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://i-lab.aegean.gr/ontologies/rrao#sentencedTo -->

    <owl:ObjectProperty rdf:about="http://i-lab.aegean.gr/ontologies/rrao#sentencedTo">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
        <rdfs:domain rdf:resource="http://i-lab.aegean.gr/ontologies/rrao#senetenced"/>
        <rdfs:range rdf:resource="http://i-lab.aegean.gr/ontologies/rrao#penalty"/>
        <rdfs:comment>Indicates the penalty to which an offender has been sentenced.</rdfs:comment>
        <rdfs:label>sentenced to</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Data properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://i-lab.aegean.gr/ontologies/rrao#ageGroup -->

    <owl:DatatypeProperty rdf:about="http://i-lab.aegean.gr/ontologies/rrao#ageGroup">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topDataProperty"/>
        <rdfs:domain rdf:resource="http://i-lab.aegean.gr/ontologies/rrao#person"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment xml:lang="en">The age group that the person belongs to.</rdfs:comment>
        <rdfs:label xml:lang="en">age group</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://i-lab.aegean.gr/ontologies/rrao#amountOfPecuniaryPenalty -->

    <owl:DatatypeProperty rdf:about="http://i-lab.aegean.gr/ontologies/rrao#amountOfPecuniaryPenalty">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topDataProperty"/>
        <rdfs:domain rdf:resource="http://i-lab.aegean.gr/ontologies/rrao#pecuniarySentence"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#monetary"/>
        <rdfs:comment xml:lang="en">Specifies the monetary amount of the pecuniary penalty.</rdfs:comment>
        <rdfs:label xml:lang="en">amount of pecuniary penalty</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://i-lab.aegean.gr/ontologies/rrao#beneficialCalculation -->

    <owl:DatatypeProperty rdf:about="http://i-lab.aegean.gr/ontologies/rrao#beneficialCalculation">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topDataProperty"/>
        <rdfs:domain rdf:resource="http://i-lab.aegean.gr/ontologies/rrao#penalty"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#boolean"/>
        <rdfs:comment xml:lang="en">Indicates whether beneficial calculation was applied to the penalty.</rdfs:comment>
        <rdfs:label xml:lang="en">beneficial calculation</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://i-lab.aegean.gr/ontologies/rrao#birthYear -->

    <owl:DatatypeProperty rdf:about="http://i-lab.aegean.gr/ontologies/rrao#birthYear">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topDataProperty"/>
        <rdfs:domain rdf:resource="http://i-lab.aegean.gr/ontologies/rrao#person"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#dateTime"/>
        <rdfs:comment>Specifies the birth year of a person.</rdfs:comment>
        <rdfs:label>birth year</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://i-lab.aegean.gr/ontologies/rrao#clildren -->

    <owl:DatatypeProperty rdf:about="http://i-lab.aegean.gr/ontologies/rrao#clildren">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topDataProperty"/>
        <rdfs:domain rdf:resource="http://i-lab.aegean.gr/ontologies/rrao#person"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#boolean"/>
        <rdfs:comment>Indicates whether the person has children.</rdfs:comment>
        <rdfs:label>children</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://i-lab.aegean.gr/ontologies/rrao#dateOfCrime -->

    <owl:DatatypeProperty rdf:about="http://i-lab.aegean.gr/ontologies/rrao#dateOfCrime">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topDataProperty"/>
        <rdfs:domain rdf:resource="http://i-lab.aegean.gr/ontologies/rrao#crime"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#dateTime"/>
        <rdfs:comment xml:lang="en">Represents the date on which the crime occurred.</rdfs:comment>
        <rdfs:label xml:lang="en">date of crime</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://i-lab.aegean.gr/ontologies/rrao#dateOfEnteringPrison -->

    <owl:DatatypeProperty rdf:about="http://i-lab.aegean.gr/ontologies/rrao#dateOfEnteringPrison">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topDataProperty"/>
        <rdfs:domain rdf:resource="http://i-lab.aegean.gr/ontologies/rrao#custodialPenalty"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#dateTime"/>
        <rdfs:comment xml:lang="en">Specifies the date when the individual entered prison.</rdfs:comment>
        <rdfs:label xml:lang="en">date of entering prison</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://i-lab.aegean.gr/ontologies/rrao#dateOfExitingPrison -->

    <owl:DatatypeProperty rdf:about="http://i-lab.aegean.gr/ontologies/rrao#dateOfExitingPrison">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topDataProperty"/>
        <rdfs:domain rdf:resource="http://i-lab.aegean.gr/ontologies/rrao#custodialPenalty"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#dateTime"/>
        <rdfs:comment xml:lang="en">Indicates the date when the individual is expected to exit prison.</rdfs:comment>
        <rdfs:label xml:lang="en">date of exiting prison</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://i-lab.aegean.gr/ontologies/rrao#daysInCustody -->

    <owl:DatatypeProperty rdf:about="http://i-lab.aegean.gr/ontologies/rrao#daysInCustody">
        <rdfs:domain rdf:resource="http://i-lab.aegean.gr/ontologies/rrao#imprisoned"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#int"/>
        <rdfs:label xml:lang="en">days in custody</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://i-lab.aegean.gr/ontologies/rrao#daysSinceRelease -->

    <owl:DatatypeProperty rdf:about="http://i-lab.aegean.gr/ontologies/rrao#daysSinceRelease">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topDataProperty"/>
        <rdfs:domain rdf:resource="http://i-lab.aegean.gr/ontologies/rrao#released"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#int"/>
        <rdfs:comment xml:lang="en">Indicates the days since the person has been released form prison</rdfs:comment>
        <rdfs:label xml:lang="en">days since release</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://i-lab.aegean.gr/ontologies/rrao#decisionCode -->

    <owl:DatatypeProperty rdf:about="http://i-lab.aegean.gr/ontologies/rrao#decisionCode">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topDataProperty"/>
        <rdfs:domain rdf:resource="http://i-lab.aegean.gr/ontologies/rrao#courtDecision"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment xml:lang="en">Specifies the code or reference associated with a court decision.</rdfs:comment>
        <rdfs:label xml:lang="en">decision code</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://i-lab.aegean.gr/ontologies/rrao#decisionDate -->

    <owl:DatatypeProperty rdf:about="http://i-lab.aegean.gr/ontologies/rrao#decisionDate">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topDataProperty"/>
        <rdfs:domain rdf:resource="http://i-lab.aegean.gr/ontologies/rrao#courtDecision"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#dateTime"/>
        <rdfs:comment xml:lang="en">Indicates the date associated with a court decision.</rdfs:comment>
        <rdfs:label xml:lang="en">decision date</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://i-lab.aegean.gr/ontologies/rrao#generalRegisterNumber -->

    <owl:DatatypeProperty rdf:about="http://i-lab.aegean.gr/ontologies/rrao#generalRegisterNumber">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topDataProperty"/>
        <rdfs:domain rdf:resource="http://i-lab.aegean.gr/ontologies/rrao#senetenced"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment xml:lang="en">Represents the general register number associated with an imprisoned person.</rdfs:comment>
        <rdfs:label xml:lang="en">general register number</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://i-lab.aegean.gr/ontologies/rrao#hasPenaltyDuration -->

    <owl:DatatypeProperty rdf:about="http://i-lab.aegean.gr/ontologies/rrao#hasPenaltyDuration">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topDataProperty"/>
        <rdfs:domain rdf:resource="http://i-lab.aegean.gr/ontologies/rrao#penalty"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#duration"/>
        <rdfs:comment xml:lang="en">Represents the duration for which the penalty is enforced.</rdfs:comment>
        <rdfs:label xml:lang="en">penalty duration</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://i-lab.aegean.gr/ontologies/rrao#irrevocableConviction -->

    <owl:DatatypeProperty rdf:about="http://i-lab.aegean.gr/ontologies/rrao#irrevocableConviction">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topDataProperty"/>
        <rdfs:domain rdf:resource="http://i-lab.aegean.gr/ontologies/rrao#penalty"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#boolean"/>
        <rdfs:comment xml:lang="en">Indicates whether the conviction is irrevocable.</rdfs:comment>
        <rdfs:label xml:lang="en">irrevocable conviction</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://i-lab.aegean.gr/ontologies/rrao#isFromConcersion -->

    <owl:DatatypeProperty rdf:about="http://i-lab.aegean.gr/ontologies/rrao#isFromConcersion">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topDataProperty"/>
        <rdfs:domain rdf:resource="http://i-lab.aegean.gr/ontologies/rrao#penalty"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#boolean"/>
        <rdfs:comment xml:lang="en">Specifies whether the penalty is the result of a conversion from another penalty.</rdfs:comment>
        <rdfs:label xml:lang="en">is from conversion</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://i-lab.aegean.gr/ontologies/rrao#isInitialSentence -->

    <owl:DatatypeProperty rdf:about="http://i-lab.aegean.gr/ontologies/rrao#isInitialSentence">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topDataProperty"/>
        <rdfs:domain rdf:resource="http://i-lab.aegean.gr/ontologies/rrao#penalty"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#boolean"/>
        <rdfs:label xml:lang="en">is initial sentence</rdfs:label>
        <rdfs:seeAlso xml:lang="en">Indicates whether the penalty is an initial sentence.</rdfs:seeAlso>
    </owl:DatatypeProperty>
    


    <!-- http://i-lab.aegean.gr/ontologies/rrao#levelOfEducation -->

    <owl:DatatypeProperty rdf:about="http://i-lab.aegean.gr/ontologies/rrao#levelOfEducation">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topDataProperty"/>
        <rdfs:domain rdf:resource="http://i-lab.aegean.gr/ontologies/rrao#person"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment>The highest degree or level of schooling that an individual has completed.</rdfs:comment>
        <rdfs:label>Level of Education</rdfs:label>
        <rdfs:seeAlso rdf:resource="https://ilostat.ilo.org/methods/concepts-and-definitions/classification-education/"/>
    </owl:DatatypeProperty>
    


    <!-- http://i-lab.aegean.gr/ontologies/rrao#locatedIn -->

