    <!-- http://i-lab.aegean.gr/ontologies/rrao#accomplice -->

    <owl:Class rdf:about="http://i-lab.aegean.gr/ontologies/rrao#accomplice">
        <rdfs:subClassOf rdf:resource="http://i-lab.aegean.gr/ontologies/rrao#levelOfParticipation"/>
